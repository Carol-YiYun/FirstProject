def findAllSmall(lst, num):
    result = []
    for ele in lst:
        if ele < num:
            result.append(ele)
    print(result)
    return result


findAllSmall([1, 2, 3], 10)  # returns [1, 2, 3]
findAllSmall([1, 2, 3], 2)  # returns [1]
findAllSmall([1, 3, 5, 4, 2], 4)  # returns [1, 3, 2]
