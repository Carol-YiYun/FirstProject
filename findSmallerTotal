def findSmallerTotal(lst, num):
    counter = 0
    for ele in lst:
        if ele < num:
            counter += ele
    print(counter)
    return counter


findSmallerTotal([1, 2, 3], 3)  # returns 3
findSmallerTotal([1, 2, 3], 1)  # returns 0
findSmallerTotal([3, 2, 5, 8, 7], 999)  # returns 25
findSmallerTotal([3, 2, 5, 8, 7], 0)  # returns 0


